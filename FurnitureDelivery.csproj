<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<!-- Constant definition, comment if IL2CPP -->
		<DefineConstants>MONO</DefineConstants>
		
		<!-- Settings -->
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>disable</Nullable>
		<AllowUnsafeBlocks>True</AllowUnsafeBlocks>
		<LangVersion>latest</LangVersion>
		<ImplicitUsings>enable</ImplicitUsings>

		<IsMono Condition="'$(DefineConstants)' != '' and $(DefineConstants.Contains('MONO'))">true</IsMono>
		
		<!-- Set framework and assembly name based on Mono or IL2CPP -->
		<TargetFramework Condition="'$(IsMono)' == 'true'">netstandard2.1</TargetFramework>
		<TargetFramework Condition="'$(IsMono)' != 'true'">net6</TargetFramework>

		<RootNamespace>FurnitureDelivery</RootNamespace>
		<AssemblyName Condition="'$(IsMono)' == 'true'">FurnitureDelivery-Mono</AssemblyName>
		<AssemblyName Condition="'$(IsMono)' != 'true'">FurnitureDelivery-IL2CPP</AssemblyName>

		<!-- Paths and conditions -->
		<S1MonoDir>C:\Games\Schedule I\S1-files\Schedule I Mono</S1MonoDir>
		<S1CPPDir>C:\Games\Schedule I\S1-files\Schedule I IL2CPP</S1CPPDir>
		<RunPostBuildEvent>OnBuildSuccess</RunPostBuildEvent>

		<S1Dir Condition="'$(IsMono)' == 'true'">$(S1MonoDir)</S1Dir>
		<S1Dir Condition="'$(IsMono)' != 'true'">$(S1CPPDir)</S1Dir>

		<S1ExeName>Schedule I.exe</S1ExeName>
		<S1ExePath>$(S1Dir)\$(S1ExeName)</S1ExePath>
		<S1ModsDir>$(S1Dir)\Mods</S1ModsDir>
	</PropertyGroup>

	<ItemGroup Label="Publicizer" Condition="'$(IsMono)' == 'true'">
		<PackageReference Include="BepInEx.AssemblyPublicizer.MSBuild" Version="0.5.0-beta.1">
			<PrivateAssets>all</PrivateAssets>
			<IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
		</PackageReference>
	</ItemGroup>

	<Target Name="PrintBuildVariables" BeforeTargets="BeforeBuild">
		<Message Text="---------- Printing Build Variables ----------" Importance="high" />
		<Message Text="IsMono: $(IsMono)" Importance="high" />
		<Message Text="DefineConstants: $(DefineConstants)" Importance="high" />
		<Message Text="TargetFramework: $(TargetFramework)" Importance="high" />
		<Message Text="S1Dir: $(S1Dir)" Importance="high" />
		<Message Text="S1ExeName: $(S1ExeName)" Importance="high" />
		<Message Text="S1ExePath: $(S1ExePath)" Importance="high" />
		<Message Text="S1ModsDir: $(S1ModsDir)" Importance="high" />
		<Message Text="TargetPath: $(TargetPath)" Importance="high" />
		<Message Text="---------------------------------------------" Importance="high" />
	</Target>

	<Target Name="PostBuild" AfterTargets="PostBuildEvent" Condition="'$(RunPostBuildEvent)' == 'OnBuildSuccess'">
		<Exec Command='(tasklist | findstr /I "Schedule.*I.exe" &gt;nul || exit 0) &amp;&amp; taskkill /F /IM "$(S1ExeName)"'/>
		<Exec Command='powershell -Command "while (Get-Process -Name \"$(S1ExeName)\" -ErrorAction SilentlyContinue) { Start-Sleep -Milliseconds 200 }; Start-Sleep -Seconds 1"' />
		<Exec Command='copy "$(TargetPath)" "$(S1ModsDir)"'/>
		<Exec Command='powershell -Command "Start-Process -FilePath \"$(S1ExePath)\""' />
	</Target>

